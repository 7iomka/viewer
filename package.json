{
  "name": "react-pages-boilerplate",
  "version": "1.0.0",
  "description": "Simple boilerplate for working with React, redux, react-router and Github Pages",
  "main": "index.js",
  "scripts": {
    "test": "npm run lint:eslint && npm run lint:stylelint && npm run test:tape",
    "test:cache": "npm run lint:eslint-cache && npm run lint:stylelint && npm run test:tape",
    "test:tape": "cross-env NODE_ENV=development tape --require babel-register --require ignore-styles src/**/*.test.{js,jsx} | tap-notify | tap-spec",
    "start": "cross-env NODE_ENV=development node scripts/start",
    "build": "cross-env NODE_ENV=production webpack --progress --colors && npm run storybook:build",
    "clean": "rimraf public",
    "lint": "npm run lint:eslint-fix && npm run lint:stylelint",
    "lint:eslint": "eslint src --ext .js --ext .jsx",
    "lint:eslint-fix": "eslint src --ext .js --ext .jsx --fix",
    "lint:eslint-cache": "eslint src --ext .js --ext .jsx --cache",
    "lint:styles": "stylelint 'src/**/*.scss' --cache",
    "pages:build": "npm run clean && npm run storybook:build && cross-env NODE_ENV=production BUILD=pages webpack --progress --colors",
    "pages:deploy": "npm run pages:build && gh-pages -d public",
    "storybook:start": "start-storybook -p 3003 -c storybook",
    "storybook:build": "build-storybook -c storybook -o public/storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rtivital/react-pages-boilerplate.git"
  },
  "keywords": [
    "react",
    "react-challenge",
    "react-router",
    "redux",
    "react-redux"
  ],
  "author": "Vitaly Rtishchev <rtivital@gmail.com> (http://github.com/rtivital)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rtivital/react-pages-boilerplate/issues"
  },
  "homepage": "https://github.com/rtivital/react-pages-boilerplate#readme",
  "dependencies": {
    "axios": "^0.16.2",
    "classnames": "^2.2.5",
    "lodash": "^4.17.2",
    "node-uuid": "^1.4.8",
    "normalize.css": "^7.0.0",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-fastclick": "^3.0.2",
    "react-onclickoutside": "^6.1.1",
    "react-redux": "^5.0.5",
    "react-router": "^3.0.5",
    "react-router-redux": "^4.0.7",
    "redux": "^3.7.1",
    "redux-actions": "^2.0.3",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.34.0",
    "@kadira/storybook-addon-knobs": "^1.7.1",
    "@kadira/storybook-addons": "^1.6.1",
    "autoprefixer": "^7.1.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-lodash": "^3.2.10",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-imports": "^1.4.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "babel-runtime": "^6.20.0",
    "chalk": "^1.1.3",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "enzyme": "^2.9.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-import-resolver-babel-module": "^3.0.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "extract-text-webpack-plugin": "^2.1.2",
    "favicons-webpack-plugin": "0.0.7",
    "file-loader": "^0.11.2",
    "gh-pages": "^1.0.0",
    "git-repo-name": "^0.6.0",
    "html-webpack-plugin": "^2.29.0",
    "ignore-styles": "^5.0.1",
    "jsonfile": "^3.0.0",
    "lodash-webpack-plugin": "^0.11.4",
    "mkdirp": "^0.5.1",
